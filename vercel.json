{
    "version": 2,
    "builds": [
        {
            "src": "nest/main.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "nest/main.js",
            "methods": [ "GET", "POST", "PUT", "PATCH", "DELETE" ]
        }
    ]
}